{
  "version": "3.5.2",
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "noImplicitAny": false,
    "noLib": false,
    "declaration": true,
    "outDir": "js",
    "esModuleInterop": true,
    "jsx": "react"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "js",
    "**/*.test.ts"
  ]
}
